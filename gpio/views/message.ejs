<div class="row"
     align="left">
  <div class="form-group"
       style="padding-left: 0%;">
    <div class="col-xs-8 col-sm-8 col-md-3 col-lg-3">

      <div class="alert alert-success"
           data-ng-show="message.containsSucessfulMessage">
        <h5> {{message.textMessage}} </h5>
      </div>
      <div class="alert alert-warning"
           data-ng-show="message.containsErrorMessage">
        <h5> {{message.textMessage}} </h5>
      </div>

    </div>
  </div>
</div>
