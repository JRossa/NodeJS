

<label id="pinInputState" hidden>
</label>

<label id="pinInputValue" hidden>
  {{pinData.pinInput}}
</label>


<div class="row"
     data-ng-show="false">
  <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
    <div class="form-group"
         style="padding-left: 0%;">

         <input type="checkbox"
         id="myInterview"
         data-checkbox-toggle="toggle"
         data-style="ios"
         data-onstyle="info"
         data-size="small"
         data-on="On"
         data-off="Off"
         ng-model="pinData.pinInput">
     </div>
   </div>
 </div>

<div class="row"
     data-ng-show="showLang.show_PT">
  <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
    <div class="form-group"
         style="padding-left: 0%;">
      <div ng-class="{'has-error': validatePinSensorId.containsValidationError}">
        <label for="pinInput">
          {{label.createmenu_pinInput}}
        </label>

        <span style="padding-left: 50px; display:inline-block;"> </span>

        <input id="pinInputPT"
               type="checkbox"
               onchange="setPinInputState()"
               data-checkbox-toggle="toggle"
               data-width="100"
               data-on={{label.createmenu_sensorLocation}}
               data-off="SaÃ­da"
               data-onstyle="success"
               data-offstyle="danger"
               data-size="small"
               ng-model="pinData.pinInput">
        </input>
      </div>
    </div>
  </div>
</div>

<div class="row"
     data-ng-show="showLang.show_EN">
  <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
    <div class="form-group"
         style="padding-left: 0%;">
      <div ng-class="{'has-error': validatePinSensorId.containsValidationError}">
        <label for="pinInput">
          {{label.createmenu_pinInput}}
        </label>

        <span style="padding-left: 50px; display:inline-block;"> </span>

        <input id="pinInputEN"
               type="checkbox"
               onchange="setPinInputState()"
               data-checkbox-toggle="toggle"
               data-width="100"
               data-on="In"
               data-off="Out"
               data-onstyle="success"
               data-offstyle="danger"
               data-size="small"
               ng-model="pinData.pinInput">
        </input>
      </div>
    </div>
  </div>
</div>
