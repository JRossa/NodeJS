<label id="pinUsedState" hidden>
</label>

<label id="pinUsedValue" hidden>
  {{pinData.pinUsed}}
</label>


<div class="row"
     data-ng-show="showLang.show_PT">
  <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
    <div class="form-group"
         style="padding-left: 0%;">
      <div ng-class="{'has-error': validatePinSensorId.containsValidationError}">
        <label for="pinUsed">
          {{label.createmenu_pinUsed}}
        </label>

        <span  style="padding-left: 10px; display:inline-block;"> </span>

        <input id="pinUsedPT"
               type="checkbox"
               onchange="setPinUsedState()"
               data-toggle="toggle"
               data-width="100"
               data-on="Ligado"
               data-off="Desligado"
               data-onstyle="success"
               data-offstyle="danger"
               data-size="small">
        </input>
      </div>
    </div>
  </div>
</div>

<div class="row"
     data-ng-show="showLang.show_EN">
  <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10">
    <div class="form-group"
         style="padding-left: 0%;">
      <div ng-class="{'has-error': validatePinSensorId.containsValidationError}">
        <label for="pinUsed">
          {{label.createmenu_pinUsed}}
        </label>

        <span style="padding-left: 80px; display:inline-block;"> </span>

        <input id="pinUsedEN"
               type="checkbox"
               onchange="setPinUsedState()"
               data-toggle="toggle"
               data-width="100"
               data-on="On"
               data-off="Off"
               data-onstyle="success"
               data-offstyle="danger"
               data-size="small">
        </input>
      </div>
    </div>
  </div>
</div>
